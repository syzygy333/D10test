# Project Ignore rules
# Drupal
/web/*.*
/web/core
/web/libraries
/web/modules/contrib
/web/themes/contrib
/web/profiles/contrib

/web/sites/*
!/web/sites/default
/web/sites/default/*
!/web/sites/default/settings.php
!web/robots.txt

/config/**/.htaccess

# Drush
/drush/contrib

# Compiled assets
/web/**/*.css
/web/**/*.js
!/web/**/*.es6.js
/web/**/*.map

# Project dependencies
/node_modules
/vendor/*
!/vendor/.rsync-filter

# Project test results
/tests/**/results
.phpunit.result.cache

# Local development
/.ddev/*
!/.ddev/config.yaml
/.gitattributes
